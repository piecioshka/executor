        "dev": "webpack-dashboard -- npm run build",
    },
    "devDependencies": {
        "ace-builds": "^1.2.2",
        "babel-core": "^6.7.7",
        "babel-eslint": "^6.1.0",
        "babel-loader": "^6.2.4",
        "babel-polyfill": "^6.7.4",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-stage-0": "^6.5.0",
        "babel-standalone": "^6.7.7",
        "css-loader": "^0.23.1",
        "eslint": "^2.13.1",
        "eslint-config-piecioshka": "^1.1.3",
        "eslint-loader": "^1.3.0",
        "gh-pages": "^0.11.0",
        "json-loader": "^0.5.4",
        "style-loader": "^0.13.1",
        "super-event-emitter": "^4.1.3",
        "webpack": "^1.13.0",
        "webpack-dashboard": "^0.1.1"
    },
    "keywords": [
        "executor",
        "highlight",
        "syntax",
        "evaluate",
        "javascript",
        "code",
        "babel",
        "ace",
        "editor",
        "embed",
        "slides",
        "presentation"
