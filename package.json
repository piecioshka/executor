        "babel-loader": "^8.0.6",
        "debounce": "^1.2.0",
        "escape-html": "^1.0.3",
        "eslint": "^6.3.0",
        "eslint-config-piecioshka": "^2.0.2",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "sass": "^1.22.10",
        "selection-range": "^1.1.0",
        "super-event-emitter": "^4.1.12",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.8",
        "webpack-dashboard": "^3.2.0"
    },
    "dependencies": {},
    "keywords": [
        "executor",
